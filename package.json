{
  "name": "councils-feathers",
  "description": "Councils rewritten in feathers js with graphql",
  "version": "0.0.1",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Viktor Shamal Andersen",
    "email": "sphinxominator@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "knex": "knex",
    "mocha": "mocha test/ --recursive",
    "start:app": "cd app && yarn start",
    "start:dev": "concurrently \"yarn run start:server\" \" yarn run start:app  \" ",
    "start:server:dev": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "start:server": "NODE_ENV=development nodemon src/index.js --exec babel-node --ignore 'app/*.js'",
    "start": " cd app && yarn build && cd ../ && NODE_ENV=production babel-node src/index.js",
    "postinstall": "cd app && yarn install",
    "webpack": "webpack-dev-server"
  },
  "dependencies": {
